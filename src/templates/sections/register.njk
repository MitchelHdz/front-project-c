{% extends "partials/base.njk" %}

{% block meta %}
{% endblock %}

{% block content %}
<div id="content" role="main" class="cierralo-content">
    <div class="register-header cierralo-flex-colum">
        <p class="register-title cierralo-font-helveltica-medium cierralo-grey-text">Registrate</p>
    </div> 
    <div class="login-divider">

    </div>
    <div class="login-social cierralo-flex-colum">
        <div class="user-options-container cierralo-flex-row">
            <div class="cierralo-flex-colum user-type-box active cursor-pointer" id="seller">
                <p class="user-type-text cierralo-font-helveltica-neue-light letter-spacing-dot-one">Vendedor</p>
            </div>
            <div class="cierralo-flex-colum user-type-box cursor-pointer" id="buyer">
                <p class="user-type-text cierralo-font-helveltica-neue-light letter-spacing-dot-one">Comprador</p>
            </div>
        </div>
    </div>
    <form>
    <div class="register-body cierralo-flex-colum">
            <div class="cierralo-flex-row-start">
                <div class="input-labels-container inline-block web-area">
                    <div class="input-label-box cierralo-flex-row-input-end">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Nombre</p>
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Apellido</p>
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Correo electrónico</p>
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Contraseña</p>
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Confirmar contraseña</p>
                    </div>
                </div>
                <div class="register-container inline-block">
                    <div class="input-label-box cierralo-flex-row-input-end mobile-input">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Nombre</p>
                    </div>
                    <div class="input-box">
                        <input type="text" name="" value="" placeholder="" class="register-input cierralo-purplish-grey cierralo-font-helveltica-neue">
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end mobile-input">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Apellido</p>
                    </div>
                    <div class="input-box">
                        <input type="text" name="" value="" placeholder="" class="register-input cierralo-purplish-grey cierralo-font-helveltica-neue">
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end mobile-input">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Correo electrónico</p>
                    </div>
                    <div class="input-box">
                        <input type="text" name="" value="" placeholder="" class="register-input cierralo-purplish-grey cierralo-font-helveltica-neue">
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end mobile-input">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Contraseña</p>
                    </div>
                    <div class="input-box">
                        <input type="text" name="" value="" placeholder="" class="register-input cierralo-purplish-grey cierralo-font-helveltica-neue">
                    </div>
                    <div class="input-label-box cierralo-flex-row-input-end mobile-input">
                        <p class="input-label-text cierralo-font-helveltica-neue cierralo-purplish-grey letter-spacing-less-two">Confirmar contraseña</p>
                    </div>
                    <div class="input-box">
                        <input type="text" name="" value="" placeholder="" class="register-input cierralo-purplish-grey cierralo-font-helveltica-neue">
                    </div>
                    <div class="login-options-container">
                        <div class="password-box">
                            <input type="checkbox" name="accept" value="" class="terms-checkbox inline-block">
                            <p class="login-password-text cierralo-purplish-grey cierralo-font-helveltica-neue inline-block letter-spacing-less-two">Recordar contraseña</p>
                        </div>
                        <button class="cierralo-font-helveltica-neue cierralo-login-btn cierralo-btn-secondary cierralo-remove-margin cursor-pointer letter-spacing-dot-six hover-btn-orange" id="loginButton">Registrate</button>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="login-or-container cierralo-flex-row">
        <div class="login-or-line-divider inline-block">

        </div>
        <div class="login-or-box inline-block">
            <p class="login-or-text cierralo-font-helveltica-neue cierralo-or-color">o</p>
        </div>
        <div class="login-or-line-divider inline-block">

        </div>
    </div>
    <div class="login-social cierralo-flex-colum">
        <div class="facebook-button cierralo-flex-row cursor-pointer">
            <img src="http://2.bp.blogspot.com/-2BMQLIQ0wY0/VL3gEjk75GI/AAAAAAAAAPM/SOjJYvW_vCM/s1600/FB-f-Logo__white_1024.png" alt="" class="login-icon-social facebook">
            <p class="button-text facebook cierralo-white-text cierralo-font-helveltica-neue letter-spacing-less-two">Iniciar sesión con Facebook</p>
        </div>
        <div class="google-button cierralo-flex-row cursor-pointer">
            <img src="http://utahseopros.com/wp-content/uploads/Google_-G-_Logo.svg_.png" alt="" class="login-icon-social google">
            <p class="button-text google letter-spacing-less-two cierralo-font-helveltica-neue">Iniciar sesión en Google </p>
        </div>
    </div>
    <div class="cierralo-pre-footer-divider cierralo-flex-row">

    </div>
</div>
{% endblock %}
