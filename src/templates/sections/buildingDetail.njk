{% extends "partials/base.njk" %}

{% block meta %}
{% endblock %}
{% block content %}
    <style type="text/css">
        body{
            opacity: 0;
            -webkit-transition: all 1s ease-out;  
            -moz-transition: all 1s ease-out;  
            -o-transition: all 1s ease-out;  
            transition: all 1s ease-out;
        }
    </style>
    <div class="building-header">
            <div class="building-parallax cursor-pointer openGalleryModal" id="openGalleryModal" imageSrc="./img/photo532577139161512309.jpg">
                
            </div>
    </div>
    <div id="galleryModal" class="modal">
        <!-- Modal content -->
        <div class="gallery-modal-content cierralo-flex-row">
            <div class="modal-close-btn-container cursor-pointer" id="closeGallery">
                <span class="close cierralo-font-helveltica-neue-light cursor-pointer"><img src="./img/icons/close-modal.png" class="close-modal-img"></span>
            </div>
            <img src="" alt="" class="gallery-modal-image" id="galleryImage">
            <div class="arrow-icon-gallery-box left cursor-pointer cierralo-flex-row" id="arrow-left">
                <img src="./img/arrow-left.png" alt="" class="arrow-icon-gallery">
            </div>
            <div class="arrow-icon-gallery-box right cursor-pointer cierralo-flex-row" id="arrow-right">
                <img src="./img/arrow-right.png" alt="" class="arrow-icon-gallery">
            </div>
        </div>
    </div>
