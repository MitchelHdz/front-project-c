                    <div class="fallback">
                      <input name="file" type="file" multiple />
                    </div>
                    <div class="dz-message dropzone-credentials-container" data-dz-message>
                      <img src="./img/icons/icono-subir-archivo.png" class="credentials-dropzone-icon">
                      <p class="credential-dropzone-text cierralo-warm-grey cierralo-font-helveltica-neue">Arrastra tu archivo</p>
                    </div>
                  </form>
              </div>
          </div>
        </div>
      </div>
